#!/bin/bash
#
# config: 2 30
# @brief: Uses Linux command to list all the files in /tmp directory.
#

# shellcheck source=/dev/null
. "$DREPORT_INCLUDE"/functions

desc="TMP file list"
file_name="tmpfilelist.log"
command="ls -lRL /tmp 2> /dev/null || true"

add_cmd_output "$command" "$file_name" "$desc"
